{
    "presets": [
      ["@babel/preset-env",
      {
        "targets": {
          "edge": "17",
          "firefox": "60",
          "chrome": "67",
          "safari": "10.1",
        },
      }],
      "@babel/preset-react"
    ],
    "plugins": [
      [
        "@babel/plugin-proposal-decorators",
        {
          "legacy": true
        }
      ],
      [
        "@babel/plugin-proposal-class-properties",
        {
          "loose": true
        }
      ],
      [
        "babel-plugin-styled-components",
        {
          "displayName": true,
          "ssr": false,
          "fileName": true,
          "pure": true
        }
      ]
    ]
  }